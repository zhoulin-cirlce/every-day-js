	<body>
		<script type="text/javascript">
			function indexOf(arr, item) {
				for(var i=0;i<arr.length;i++){
			        if(arr[i]==item){
			            return i;
			        }
			    }
			    return -1;
			}
			console.log("index=",indexOf([ 1, 2, 3, 4 ], 3))
		</script>
	</body>
</html>
