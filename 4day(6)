	<head>
		<meta charset="utf-8" content="none">
	</head>
	<body>
		<script type="text/javascript">
    //题目：统计数组 arr 中值等于 item 的元素出现的次数			
    function count(arr, item) {
				var count=0;
			    for(var i=0;i<arr.length;i++){
			        if(arr[i]==item){
			            count++;
			        }
			    }
			    return count;
			}
			console.log("count=",count([1,2,3,4,'a',33,3,6,3,'b'],3))
		</script>
	</body>
</html>
